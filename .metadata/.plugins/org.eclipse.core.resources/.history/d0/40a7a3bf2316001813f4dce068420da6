import java.io.*;
import java.util.*;

public class PashMark {
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
		int x =sc.nextInt(),y=sc.nextInt(),x1=0,y1=0;
		int in = sc.nextInt();
		
		if(in == x)y1=sc.nextInt();
		else x1= in;
		
		int d = Math.max((int) Math.sqrt((x-x1)*(x-x1)),(int) Math.sqrt((y-y1)*(y-y1)));
	}
}
